{
  "name": "generator-apifire-server",
  "version": "0.2.1",
  "description": "Generates an express server complete with validation, error handling, and logging written in Typescript",
  "homepage": "https://github.com/theogravity/apifire-server",
  "author": {
    "name": "Theo Gravity",
    "email": "theo@suteki.nu",
    "url": ""
  },
  "files": [
    "generators"
  ],
  "bugs": {
    "url": "https://github.com/theogravity/apifire-server/issues"
  },
  "main": "generators/index.js",
  "keywords": [
    "openapi",
    "api",
    "typescript",
    "express",
    "yeoman-generator",
    "knex",
    "objection"
  ],
  "devDependencies": {
    "yeoman-test": "^6.3.0",
    "yeoman-assert": "^3.1.1",
    "eslint": "^8.16.0",
    "prettier": "^2.6.2",
    "husky": "^8.0.1",
    "lint-staged": "^12.4.3",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-config-xo": "^0.41.0",
    "jest": "^28.1.0"
  },
  "engines": {
    "npm": ">= 4.0.0"
  },
  "dependencies": {
    "chalk": "^4.1.2",
    "yeoman-generator": "^5.6.1",
    "yosay": "^2.0.2"
  },
  "jest": {
    "testEnvironment": "node"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ],
    "*.json": [
      "prettier --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "eslintConfig": {
    "extends": [
      "xo",
      "prettier"
    ],
    "env": {
      "jest": true,
      "node": true
    },
    "rules": {
      "prettier/prettier": "error"
    },
    "plugins": [
      "prettier"
    ]
  },
  "scripts": {
    "pretest": "eslint .",
    "test": "jest"
  },
  "repository": "theogravity/apifire-server",
  "license": "MIT"
}
